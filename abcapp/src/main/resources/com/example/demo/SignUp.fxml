<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@customer.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.SignUpController">
   <VBox alignment="CENTER" layoutX="-4.0" prefHeight="456.0" prefWidth="207.0" styleClass="vbox">
      <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#BackToHome" prefHeight="242.0" prefWidth="207.0" text="&lt;  Back To Login" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </VBox>
   <VBox alignment="CENTER" layoutX="290.0" layoutY="34.0" prefHeight="318.0" prefWidth="207.0" style="-fx-spacing: 10;">
      <TextField fx:id="firstName" promptText="First Name" />
      <TextField fx:id="lastName" promptText="Last Name" />
      <TextField fx:id="address" promptText="Address" />
      <TextField fx:id="age" promptText="Age" />
      <TextField fx:id="email" promptText="Email" />
      <TextField fx:id="password" promptText="Password" />
      <Button mnemonicParsing="false" onAction="#SignUp" prefHeight="26.0" prefWidth="232.0" text="Sign Me Up !" textFill="WHITE" />
   </VBox>
   <Label fx:id="SignUpResultLabel" contentDisplay="CENTER" layoutX="252.0" layoutY="352.0" prefHeight="18.0" prefWidth="283.0" textAlignment="CENTER">
      <font>
         <Font name="System Bold" size="13.0" />
      </font>
   </Label>
</AnchorPane>
